      "targets": [
        {
          "alias": "Num Errors",
          "database": "\"$Database\"",
          "datasource": {
            "uid": "${grafana-timestream-datasource}"
          },
          "dsType": "grafana-timestream-datasource",
          "measure": "http_req_failed",
          "policy": "default",
          "rawQuery": "SELECT BIN(time, $__interval) AS time, AVG($__measure) AS $__measure_per_s\nFROM (\n  SELECT success.time, ROUND(AVG(fail_ratio.$__measure * success.http_reqs), 0) AS $__measure\n  FROM (\n    SELECT BIN(time, 1s) AS time, SUM(measure_value::double) AS http_reqs, instance_id\n    FROM $__database.$__table\n    WHERE measure_name = 'http_reqs'\n    AND $__timeFilter\n    AND $Filter LIKE '$FilterValue'\n    GROUP BY BIN(time, 1s), instance_id\n    ORDER BY BIN(time, 1s) DESC\n  ) AS success,\n  (\n    SELECT BIN(time, 1s) AS time, AVG(measure_value::double) AS $__measure, instance_id\n    FROM $__database.$__table\n    WHERE measure_name = '$__measure'\n    AND $__timeFilter\n    AND $Filter LIKE '$FilterValue'\n    GROUP BY BIN(time, 1s), instance_id\n    ORDER BY BIN(time, 1s) DESC\n  ) AS fail_ratio\n  WHERE fail_ratio.time = success.time\n  GROUP BY success.time\n)\nGROUP BY BIN(time, $__interval)",
          "refId": "A",
          "resultFormat": "time_series",
          "table": "\"$Table\"",
          "tags": [],
          "waitForResult": true
        }
      ],
      "title": "Errors Per Second",
      "type": "timeseries"
    },
